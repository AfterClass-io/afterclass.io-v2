{
  "enabled": true,
  "extends": ["config:best-practices", ":gitSignOff"],
  "automerge": false,
  "bumpVersion": "patch",
  "commitBodyTable": true,
  "configMigration": true,
  "labels": ["skip-changelog", "dependencies"],
  "packageRules": [
    {
      "enabled": false,
      "matchPackageNames": [
        "*"
      ]
    }
  ],
  "vulnerabilityAlerts": {
    "enabled": true
  },
  "osvVulnerabilityAlerts": true
}
